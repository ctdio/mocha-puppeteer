{
  "name": "mocha-puppeteer",
  "version": "0.5.1",
  "description": "Run tests in headless chrome",
  "main": "index.js",
  "bin": {
    "mocha-puppeteer": "./bin/mocha-puppeteer"
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "nyc ava test/unit test/integration",
    "test:unit": "nyc ava test/unit",
    "test:integration": "nyc ava test/integration",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "author": "Charlie Duong",
  "license": "MIT",
  "dependencies": {
    "babel-plugin-istanbul": "^4.1.4",
    "bluebird": "^3.5.0",
    "koa": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-mount": "^3.0.0",
    "koa-path-router": "^0.1.1",
    "koa-static": "^4.0.1",
    "lasso": "^2.11.16",
    "lasso-babel-transform": "^1.0.1",
    "marko": "^4.4.26",
    "mocha": "^3.5.0",
    "puppeteer": "^0.9.0",
    "rimraf": "^2.6.1",
    "superagent": "^3.6.0"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "coveralls": "^2.13.1",
    "eslint": "^4.5.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-plus": "0.0.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "git-hooks": "^1.1.8",
    "nyc": "^11.1.0",
    "proxyquire": "^1.8.0",
    "require-self-ref": "^2.0.1",
    "sinon": "^3.2.1",
    "uuid": "^3.1.0"
  },
  "ava": {
    "require": [
      "require-self-ref"
    ]
  }
}
